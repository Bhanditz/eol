      %tr{ :align => "center" }
        %th
          
        %th
          Species Profile Model
        %th
          No. of data objects

    %tbody
    
    = @arr["data_objects_count_per_category"]

    %tr
      %td{ :colspan => "5" }
        /= will_paginate @partner_curated_objects


