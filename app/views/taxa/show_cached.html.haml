- cache(:controller=>'/taxa',:part=>'page_' + @taxon_concept.id.to_s + '_' + current_user.language_abbr + '_' + current_user.expertise.to_s + '_' + current_user.vetted.to_s + '_' + current_user.default_taxonomic_browser.to_s + '_' + @taxon_concept.show_curator_controls?.to_s)  do
  = render :partial=>'show'
    
- if @cached
  %script{ :type => "text/javascript" }
    document.observe("dom:loaded", function() {
    if ($('data_objects_to_log').value != '') {
    eol_log_data_objects_for_taxon_concept( $('taxon_concept_to_log').value, $('data_objects_to_log').value );
    }
    });
