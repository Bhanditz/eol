%h3= I18n.t(:taxon_citizen_science_header)
.section
  - unless @contents.blank?
    = render :partial => 'taxa/details/category_content', :locals => { :content => @contents }
  - else
    .empty
      %p= I18n.t(:taxon_citizen_science_resources_empty)
      %p= link_to I18n.t(:add_citizen_science_resources_to_page_link_button), new_taxon_data_object_path(@taxon_concept, :toc => @add_article_toc_id), :class => 'button'
