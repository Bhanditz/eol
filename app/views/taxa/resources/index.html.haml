- content_for :social_sharing do
  = render :partial => 'shared/social_sharing'
#taxon_resources
  = render :partial => 'menu'
  .main_container
    .copy
      %h3= I18n.t(:about_resources_title)
      .empty.about
        %p= I18n.t(:about_resources_body)
        .button
          = link_to I18n.t(:about_resources_button), new_taxon_data_object_path(@taxon_concept, :link => true), :class => 'button'
